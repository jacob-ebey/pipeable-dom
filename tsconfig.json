{
	"compilerOptions": {
		"strict": true,
		"lib": ["DOM", "DOM.Iterable", "ES2022"],
		"types": ["vite/client"],
		"module": "Node16",
		"target": "ES2022",
		"moduleResolution": "Node16",
		"isolatedModules": true,
		"declaration": true,
		"noEmit": true,
		"baseUrl": "./",
		"jsx": "react-jsx",
		"jsxImportSource": "pipeable-dom",
		"paths": {
			"pipeable-dom": ["./src/pipeable-dom.ts"],
			"pipeable-dom/jsx": ["./src/jsx.ts"],
			"pipeable-dom/jsx-runtime": ["./src/jsx.ts"],
			"pipeable-dom/jsx-dev-runtime": ["./src/jsx.ts"]
		}
	}
}
